{
  "arch": "powerpc",
  "cpu": "750",
  "data-layout": "E-m:e-p:32:32-Fn32-i64:64-n32",
  "dynamic-linking": true,
  "env": "newlib",
  "executables": true,
  "has-rpath": true,
  "llvm-target": "powerpc-unknown-eabi",
  "linker": "powerpc-eabi-gcc",
  "linker-flavor": "gcc",
  "linker-is-gnu": true,
  "os": "rvl-ios",
  "pre-link-args": {
    "gcc": [
      "-r",
      "-e",
      "_prolog",
      "-u",
      "_prolog",
      "-u",
      "_epilog",
      "-u",
      "_unresolved",
      "-Wl,--gc-sections",
      "-nostdlib",
      "-g",
      "-mno-sdata",
      "-mgcn",
      "-DGEKKO",
      "-mcpu=750",
      "-meabi",
      "-mhard-float"
    ]
  },
  "panic-strategy": "abort",
  "relocation-model": "static",
  "target-endian": "big",
  "target-family": "unix",
  "target-mcount": "_mcount",
  "target-c-int-width": "32",
  "target-pointer-width": "32",
  "vendor": "nintendo"
}
